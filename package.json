{
  "name": "parallax",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "author": "Roman Klymashenko (https://github.com/semklim)",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "preview": "vite preview --open",
    "build:static": "vite build --base=./",
    "clean": "rimraf out dist coverage",
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix",
    "prettier": "prettier --write ./src",
    "lint:format": "prettier  --log-level warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "lint": "npm run lint:format && npm run lint:fix",
    "check-types": "tsc --noEmit --pretty"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-unused-imports": "^3.2.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.6",
    "sass": "^1.77.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "typescript-plugin-css-modules": "^5.1.0",
    "vite": "^5.2.11",
    "vite-tsconfig-paths": "^4.3.2"
  }
}
